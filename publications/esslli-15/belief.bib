keywords = {Humans,Problem Solving,Reaction Time,Reading,Space Perception,Time Perception},
month = oct,
number = {7},
pages = {1225--47},
pmid = {16194956},
title = {{Effects of directionality in deductive reasoning: II. Premise integration and conclusion evaluation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16194956},
volume = {58},
year = {2005}
}

