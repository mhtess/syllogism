month = sep,
number = {5},
pages = {1393--402},
pmid = {23565791},
title = {{The SDT model of belief bias: complexity, time, and cognitive ability mediate the effects of believability.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23565791},
volume = {39},
year = {2013}
}
