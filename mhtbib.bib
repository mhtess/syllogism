@article{Smith2002a,
abstract = {By the age of 3, children easily learn to name new objects, extending new names for unfamiliar objects by similarity in shape. Two experiments tested the proposal that experience in learning object names tunes children's attention to the properties relevant for naming--in the present case, to the property of shape--and thus facilitates the learning of more object names. In Experiment 1, a 9-week longitudinal study, 17-month-old children who repeatedly played with and heard names for members of unfamiliar object categories well organized by shapeformed the generalization that only objects with ith similar shapes have the same name. Trained children also showed a dramatic increase in acquisition of new object names outside of the laboratory during the course of the study. Experiment 2 replicated these findings and showed that they depended on children's learning both a coherent category structure and object names. Thus, children who learn specific names for specific things in categories with a common organizing property--in this case, shape--also learn to attend to just the right property--in this case, shape--for learning more object names.},
author = {Smith, Linda B and Jones, Susan S and Landau, Barbara and Gershkoff-Stowe, Lisa and Samuelson, Larissa},
file = {:C$\backslash$:/Users/Henry/Documents/CLASS/PSYCH\_211\_devo/smith\_objectnaming\_2002.pdf:pdf},
issn = {0956-7976},
journal = {Psychological science},
keywords = {Attention,Female,Humans,Infant,Male,Random Allocation,Verbal Learning,Vocabulary,Work},
month = jan,
number = {1},
pages = {13--9},
pmid = {11892773},
title = {{Object name learning provides on-the-job training for attention.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11892773},
volume = {13},
year = {2002}
}
@incollection{Suomi1999,
annote = {Rheses macaques: first stay witth mother, then exploire, then play with peers -- learn social heirarchies through infant play. At puberty, males leave and join with other males (so called "gangs") and emigrate to another troop -- mortality rate very high . 50\%. Females stay and develop social community -- have babies.

        
Individual differences. (or subgroups)
1. High-reactive monkeys -- leave mothers later, explore less, shy and withdrawn 
2. High-impulse monkeys -- often permnanently expelled from natal troop prior to puberty; engage in risk taking behavior;

        
Monkeys raised by peers;
- look normal, but exploratory behavior is limited (anxious)
- make more generally highly reactive than might have been if raised by mothers
- persists, end up at bottom of hierarchy, grouped with very young children
- makes males highly impulsive (females too, get groomed less) and lower serotonergic funcitoning
- peer reared mothers more likely to be neglectful and/or abusive for 1st born (much better than mothers raised in isolation; and does improve after 1stborn)

        
At risk high-reactive infants raised by nurturant females appeared behaviorally precocious; unusually secure in their attachment relationship; also transcends generation, those who become mothers carry this tradition forward},
